{
  "compilerOptions": {
    "module": "commonjs",
    "declaration": true,
    "removeComments": true,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "allowSyntheticDefaultImports": true,
    "target": "ES2021",
    "sourceMap": true,
    "outDir": "./dist",
    "baseUrl": "./",
    "incremental": true,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "paths": {
      "defaultLibraryPrefix/tts": [
        "libs/azure/src"
      ],
      "defaultLibraryPrefix/tts/*": [
        "libs/azure/src/*"
      ]
    }
  }
}
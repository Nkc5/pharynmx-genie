{
  "moduleFileExtensions": [
    "js",
    "json",
    "ts"
  ],
  "rootDir": ".",
  "testEnvironment": "node",
  "testRegex": "\\.e2e-spec\\.ts$",
  "transform": {
    "^.+\\.(t|j)s$": "ts-jest"
  },
  "moduleNameMapper": {
    "defaultLibraryPrefix/tts/(.*)": "<rootDir>/../libs/tts/src/$1",
    "defaultLibraryPrefix/tts": "<rootDir>/../libs/tts/src"
  }
}